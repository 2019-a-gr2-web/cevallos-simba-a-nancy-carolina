<%- include('../Componentes/menu/menu')%>
<style>
    table,tr,td,th{
        border: solid black 1px;
    }

</style>
<a href="/api/traguito/crear">
    <button>Crear</button>
</a>
<table>
    <tr>
        <th>Nombre</th>
        <th>Grados</th>
        <th>Caduca</th>
        <th>Precio</th>
        <th>Tipo</th>
        <th>Acciones</th>
    </tr>
    <% arregloTragos.forEach((trago)=>{ %>
    <tr>
        <td> <%=trago.nombre%> </td>
        <td> <%=trago.gradoAlcohol%> </td>
        <td> <%=trago.fechaCaducidad%> </td>
        <td> <%=trago.precio%> </td>
        <td> <%=trago.tipo%> </td>
        <td> <button>Editar</button>
            <form action="/api/traguito/eliminar" method="post">
                <input type="text" id="tragoId" name="id" hidden value="<%=trago.id%> ">
                <button type="submit">Eliminar</button>
            </form>

        </td>
    </tr>
    <% })%>
</table>

<%- include('../Componentes/footer/footer')%>